@model List<Category>

<div class="container">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">
                Category List
            </h2>
        </div>
        <div class="col-6">
        </div>
    </div>
    <table class=" table table-bordered table-striped">
        <thead>
            <tr>
                <th>
                    Category Name
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var obj in Model)
            {
                <tr>
                    <td>
                        @obj.Name
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>